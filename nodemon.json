{
    "restartable": "rs",
    "ignore": [".git", "node_modules/**/node_modules", "logs/", "dist/"],
    "verbose": true,
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "ts,js,html,json,yml,css",
    "exec": "npm run build && npm run lint && node --harmony ./dist/server.js",
    "delay": "2500"
}
